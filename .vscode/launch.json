{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Flutter",
        "request": "launch",
        "type": "dart",
        "program": "lib/main.dart",
        "args": [
          "--no-sound-null-safety",
          "run",
          "--no-sound-null-safety"
        ],
        "cwd": "${workspaceFolder}",
        "serverReadyAction": {
          "action": "openExternally",
          "pattern": "\\s+An Observatory\\s+\\(\\S+\\)\\s+is\\s+available\\s+at\\s+(\\S+)"
        },
        "console": "terminal"
      }
    ]
  }
  